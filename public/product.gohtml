<script src="/js/bootstrap-affix.js"></script>
<script>
    _386 = {
        fastLoad: false ,
        onePass: false ,
        speedFactor: 4
    };


    function ThisYear() {
        document.getElementById('thisyear').innerHTML = new Date().getFullYear();
    };
</script>
